<?xml version="1.0"?>
<extension key="com.example.osdi" type="module">
  <file>osdi</file>
  <name>OSDI Contact Sync</name>
  <description>An application for syncing CiviCRM contacts through the OSDI format.</description>
  <license>AGPL-3.0</license>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <maintainer>
    <author>Andy Gu</author>
    <email>andygu@usc.edu</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/4ndygu/civicrm_osdi</url>
    <url desc="Documentation">https://github.com/4ndygu/civicrm_osdi/blob/master/docs/index.md</url>
    <url desc="Support">https://github.com/4ndygu/civicrm_osdi/blob/master/docs/index.md</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2018-04-27</releaseDate>
  <version>1.0</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>4.7</ver>
  </compatibility>
  <comments>This is a new, undeveloped module</comments>
  <civix>
    <namespace>CRM/Osdi</namespace>
  </civix>
</extension>
